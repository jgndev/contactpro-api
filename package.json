{
  "name": "contactpro-api",
  "version": "1.0.0",
  "description": "Backend API for Contact Pro built with Node.js, Express and TypeScript",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Testing has not been implemented yet...\" && exit 1",
    "build": "tsc",
    "start": "tsc && node ./dist/app.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'npm run build && node ./dist/app.js'",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:migrate": "npm run typeorm -- migration:run --config ormconfig.js",
    "typeorm:generate": "npm run typeorm -- migration:generate --config ormconfig.js",
    "typeorm:revert": "npm run typeorm -- migration:revert --config ormconfig.js"
  },
  "keywords": [],
  "author": "Jeremy Novak",
  "license": "MIT",
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.11.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
